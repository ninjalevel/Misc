function Export-ProjectScriptResults {
    [CmdletBinding()]
    param (
        # ExportData
        [Parameter()]
        [SWITCH]
        $ExportData,
        # AppendData
        [Parameter()]
        [switch]
        $AppendData,
        # ClientName
        [Parameter(Mandatory)]
        [STRING]
        $ClientName,
        # ProjectName
        [Parameter(Mandatory)]
        [STRING]
        $ProjectName,
        # ReportName
        [Parameter(Mandatory)]
        [STRING]
        $ReportName,
        # InputData
        [Parameter(Mandatory)]
        $InputData

    )

    begin {
        $Timestamp = get-date -Format ("yyyy-MM-dd HH.mm")
        $OutputDir = "$($env:USERPROFILE)\documents\$($CLIENTNAME)\$($PROJECTNAME)\Reports"
        $OutputFilepath = "$($OUTPUTDIR)\$($ReportName)_$TIMESTAMP.csv"
        If (!(test-path $OutputDir)) { New-Item -ItemType Directory -Force -Path $OUTPUTDIR }
    }

    process {
        if ($ExportData) {
            IF ($AppendData) { $InputData | Export-Csv -NoTypeInformation -append $OutputFilepath }
            else { $InputData | Export-Csv -NoTypeInformation -append $OutputFilepath }
        }

    }

    end {

        Start-Process $OutputDir
    }
}
# Export-ProjectScriptResults -ExportData -ClientName 'AUMA' -ProjectName 'SMB' -ReportName 'CheckWSman' -InputData $InputData
